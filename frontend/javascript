## Object Example ##
	let person = {
	  firstName:"Jack",
	  lastName:"Norris",
	  fullName: function() {
	    return this.firstName + " " + this.lastName;
	    }
	  };

	  	person.firstName;
		person["firstName"];
		person.fullName();

## JSON Example ##
	{
	  "movies" : {
	    "movie1" : {
	      title" : "Star Wars",
	      "subtitle" : "the force awakens"
	      },
	    "movie2" : {
	      "title" : "Star Wars",
	      "subtitle" : "the empire strikes back"
	    }
	  }
	}

